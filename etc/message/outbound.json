[
    {
        "$class": "uuid",
        "service": "couchdb",
        "$set": "uuid"
    },
    {
        "$class": "message",
        "id": "{$uuid}",
        "outbound": true,
        "data": "{$job.data}",
        "$set": "msgObj"
    },
    {
        "$class": "message",
        "$method": "as_logable",
        "message": "{$msgObj}",
        "$set": "loggable_msg"
    },
    {
        "$class": "couch",
        "doc": "{$loggable_msg}",
        "$set": "msg_log"
    },
    {
        "$function": "console.print",
        "$args": [ "Msg log: ", "{$msg_log}" ]
    },
    {
        "if": "{$msg_log}",
        "$class": "message",
        "$method": "attach_body",
        "retries": 2,
        "important": true,
        "message": "{$msgObj}",
        "$set": "attached_msg_ready"
    },
    {
        "if": "{$attached_msg_ready}",
        "$class": "couch",
        "$method": "get",
        "id": "{$uuid}",
        "$set": "msg"
    },
    {
        "$class": "job",
        "name": "send-{$msg.channel}",
        "data": "{$msg}",
        "$set": "send_job"
    },
    {
        "$class": "util",
        "$method": "set",
        "base":  "{$msg_log}",
        "name":  [ "ok", "send_job_id" ],
        "value": [ true, "{$send_job}" ],
        "$set": "log"
    },
    {
        "$class": "util",
        "$method": "set",
        "name":[ "id", "status", "sendId" ],
        "value":[ ["{$uuid}"], ["send_job"], ["{$send_job}"] ],
        "$set": "log_job_data"
    },
    {
        "$class": "job",
        "name": "{$msg.channel}-send-notify",
        "data": "{$log_job_data}"
    }
]