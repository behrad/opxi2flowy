[
    {
        "$class": "atm_event",
        "$method": "event_branch",
        "eventId": "{$job.data.id}",
        "$set": "branch"
    },
    {
        "$class": "crm",
        "$method": "cells",
        "sort_by": "customfields.alarm_priority",
        "query": "org_unit",
        "value": "{$branch.id}",
        "$set": "branch_cells",
        "$empty": "no_cells"
    },
    {
        "$class": "crm",
        "$method": "event_matches",
        "label": "worktime",
        "$set": "at_work",
        "$empty": "off_work"
    },
    {
        "if": "{$off_work}",
        "$class": "crm",
        "$method": "homes",
        "sort_by": "customfields.alarm_priority",
        "query": "org_unit",
        "value": "{$branch.id}",
        "$set": "branch_phones",
        "$empty": "no_phones"
    },
    {
        "if": [ "{$off_work}" ],
        "$class": "job",
        "$method": "log",
        "job": "{$job}",
        "format": "Off work, try to add home phone numbers"
    },
    {
        "if": "{$at_work}",
        "$class": "crm",
        "$method": "phones",
        "sort_by": "customfields.alarm_priority",
        "query": "org_unit",
        "value": "{$branch.id}",
        "$set": "branch_phones",
        "$empty": "no_phones"
    },
    {
        "if": [ "{$at_work}" ],
        "$class": "job",
        "$method": "log",
        "job": "{$job}",
        "format": "Off work, try to add branch phone numbers"
    },
    {
        "if": [ "{$no_cells}", "{$no_phones}" ],
        "$class": "util",
        "$method": "set",
        "name":["error", "message"],
        "value":[true, "No Contact Addresses"],
        "$set": "alarm_answered"
    },
    {
        "if": [ "{$no_cells}", "{$no_phones}" ],
        "$class": "job",
        "$method": "log",
        "job": "{$job}",
        "format": "No Contact Addresses"
    },
    {
        "if": [ "{$branch_cells}", "{$branch_phones}"],
        "$function": "Array.prototype.concat",
        "$args": [ "{$branch_cells}", "{$branch_phones}" ],
        "$set": "branch_contacts"
    },
    {
        "if": [ "{$no_cells}", "{$branch_phones}"],
        "$function": "Array.prototype.concat",
        "$args": [ "{$branch_phones}" ],
        "$set": "branch_contacts"
    },
    {
        "if": [ "{$branch_cells}", "{$no_phones}"],
        "$function": "Array.prototype.concat",
        "$args": [ "{$branch_cells}" ],
        "$set": "branch_contacts"
    },
    {
        "if": "{$branch_contacts}",
        "$class": "message",
        "$method": "as_alarm_msg",
        "alarm_id": "{$job.data.id}",
        "media_url": "{$job.data.alarm.media_url}",
        "destinations": "{$branch_contacts}",
        "answer_timeout": 240,
        "$set": "multi_msg_alarm"
    },
    {
        "if": "{$multi_msg_alarm}",
        "$class": "atm_event",
        "$method": "event_atm",
        "eventId": "{$job.data.id}",
        "$set": "atm"
    },
    {
        "$class": "atm_event",
        "id": "{$job.data.id}",
        "$set": "event"
    },
    {
        "$class": "moment",
        "$method": "time_str",
        "date": "{$event.failure_time}",
        "format": "HH:mm",
        "$set":"time"
    },
    {
        "$class": "moment",
        "$method": "jalali_str",
        "date": "{$event.failure_time}",
        "$set":"jalali_date"
    },
    {
        "$class": "util",
        "$method": "set",
        "base": "{$event}",
        "name":[ "time", "jalali_date" ],
        "value":[ "{$time}", "{$jalali_date}" ],
        "$set": "pre_ready"
    },
    {
        "if": "{$pre_ready}",
        "$class": "atm_event",
        "$method": "set_failure_messages",
        "event": "{$event}",
        "$set": "ready"
    },
    {
        "if": "{$ready}",
        "$class": "util",
        "$method": "set",
        "name":[ "atm", "event" ],
        "value":[ "{$atm}", "{$event}" ],
        "$set": "context_data"
    },
    {
        "$class": "util",
        "$method": "set",
        "base": "{$multi_msg_alarm}",
        "name":["__context_data", "dont_attach" ],
        "value":["{$context_data}", true ],
        "$set": "final_msg"
    },
    {
        "if": [ "{$final_msg}" ],
        "$class": "atm_event",
        "$method": "update",
        "eventId":"{$job.data.id}",
        "status":"sending alarm"
    },
    {
        "if": [ "{$final_msg}" ],
        "$class": "job",
        "$method": "log",
        "job": "{$job}",
        "format": "Create notify job to list of %s destinations",
        "args": "{$branch_contacts.length}"
    },
    {
        "if": [ "{$final_msg}" ],
        "$class": "job",
        "name": "outbound-notify-first",
        "blocking": true,
        "$retries": "{$branch_contacts.length}",
        "$priority": "{$job._priority}",
        "data": "{$final_msg}",
        "$set": "alarm_answered"
    }
]